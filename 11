function convertDate(dateStr) {
    // dateStr example: "03-11-2025"
    const parts = dateStr.split("-");
    const day = parts[0];
    const month = parts[1];
    const year = parts[2].slice(-2); // get last 2 digits

    return `${day}-${month}-${year}`;
}

console.log(convertDate("03-11-2025")); // output: 03-11-25
